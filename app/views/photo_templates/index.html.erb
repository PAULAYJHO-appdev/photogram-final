<h1>
  List of photos
</h1>

<table border="1">
  <tr>
    <th>
    Image
    </th>
    <th>
    Caption
    </th>
    <th>
    Owner
    </th>
    <th>
    Posted
    </th>
    <th>
    Likes
    </th>
    <th>
    </th>
  </tr>

<% @photos.each do |photo|%>
<tr>
  <td>
    <img src="<%= photo.image %>">
  </td>
  <td>
    <%= photo.caption %>
  </td>
  <td>
    <%= photo.owner_id%>
  </td>
  <td>
    <%= photo.created_at%>
  </td>
  <td>
    <%= photo.likes_count%>
  </td>
  <td>
    <a href="/user_sign_in/">
    Show details
    </a>
  </td>
</tr>

<% end %>
</table>
